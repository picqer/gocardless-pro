{
    "name": "picqer/gocardless-pro",
    "description": "A PHP client for the GoCardless Pro API",
    "keywords": ["GoCardless", "GoCardless Pro", "Direct Debit"],
    "license": "MIT",
    "homepage": "https://github.com/picqer/gocardless-pro",
    "authors": [
        {
            "name": "Cristian Giordano",
            "email": "developers@sneekdigital.co.uk"
        },
        {
            "name": "Stephan Groen",
            "email": "info@picqer.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "GoCardless\\Pro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoCardless\\Pro\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
